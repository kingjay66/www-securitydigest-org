
set title "tcp-ip mailing list: security related messages by month"
set xlabel "Year/Month"
set ylabel "Number of Messages"
set yrange [0:750]
plot '../l/look_security.stats.messages' using 1:2 title "Security" with linespoints,\
     '../l/look_security.stats.messages' using 1:3 title "All" with linespoints
pause -1 "Hit return to continue"

